{
	"$schema": "http://opencode.ai/config.json",
	"theme": "system",
	"model": "openrouter/openai/gpt-4.1-mini",
	"autoshare": false,
	"autoupdate": false,
	"mcp": {
		"context7": {
			"type": "local",
			"command": ["bunx", "@upstash/context7-mcp"],
			"enabled": true
		},
		"svelte": {
			"type": "local",
			"command": ["bunx", "@sveltejs/mcp"],
			"enabled": true
		}
	}
}
